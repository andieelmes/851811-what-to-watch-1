{
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./dist",
    "sourceMap": true,
    "module": "commonjs",
    "target": "es2016",
    "jsx": "react",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "src",
    "paths": {
      "App/*": [ "src/*" ]
    }
  },
  "exclude": ["node_modules", "dist"]
}
